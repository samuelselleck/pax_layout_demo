
<Frame
 @click=self.clicked
 width={self.size}
 height={self.size}
 x=50%
 y=50%
 anchor_x=50%
 anchor_y=50%
>
	<Marker x={self.p_x} y={self.p_y}/>
  	<Rectangle
	  width=20%
		height=20%
		x={self.p_x}
		y={self.p_y}
		anchor_x={self.a_x}
		anchor_y={self.a_y}
	  fill={Fill::Solid(Color::rgb(0.4, 0.4, 0.4))}/>
</Frame>

@handlers {
    did_mount: self.handle_did_mount
}